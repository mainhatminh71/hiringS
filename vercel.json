{
  "version": 2,
  "buildCommand": "node scripts/replace-env.js && npm run build:ssr",
  "outputDirectory": "dist/hiring-s/browser",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.mjs"
    },
    {
      "source": "/((?!google[a-z0-9]+\\.html).*)",
      "destination": "/api/index.mjs"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/google.*\\.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}

